    {
      "cell_type": "code",
      "source": [
        "def predict_next_word(model, tokenizer, input_text, n_steps, temperature=1.0):\n",
        "    input_seq = tokenizer.texts_to_sequences([input_text])[0]\n",
        "    input_seq = pad_sequences([input_seq], maxlen=n_steps)\n",
        "\n",
        "    predicted = model.predict(input_seq, verbose=0)[0]\n",
        "\n",
        "    # Apply temperature scaling\n",
        "    exp_predicted = np.exp(predicted / temperature)\n",
        "    predicted_probs = exp_predicted / np.sum(exp_predicted)\n",
        "\n",
        "    predicted_word_index = np.random.choice(np.arange(total_words), p=predicted_probs)\n",
        "    predicted_word = tokenizer.index_word[predicted_word_index]\n",
        "    return predicted_word"
      ],
      "metadata": {
        "id": "KAEqPs6WKRCm"
      },
      "execution_count": 34,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def predict_next_word(model, tokenizer, input_text, n_steps):\n",
        "    input_seq = tokenizer.texts_to_sequences([input_text])[0]\n",
        "    input_seq = pad_sequences([input_seq], maxlen=n_steps)\n",
        "\n",
        "    predicted = model.predict(input_seq, verbose=0)\n",
        "    predicted_word_index = np.random.choice(np.arange(total_words), p=predicted[0])  # Sample from probabilities\n",
        "    predicted_word = tokenizer.index_word[predicted_word_index]\n",
        "    return predicted_word\n"
      ],
      "metadata": {
        "id": "vBcmXL7l56VH"
      },
      "execution_count": 41,
      "outputs": []
    },
